{
  "name": "ems-core",
  "version": "2.16.14",
  "private": true,
  "main": "build/electron.js",
  "dependencies": {
    "@the-orange-alliance/lib-ems": "^0.8.8",
    "axios": "^0.19.0",
    "dotenv": "^8.0.0",
    "local-ipv4-address": "0.0.2",
    "moment": "^2.24.0",
    "opn": "^6.0.0",
    "pm2": "^3.5.0",
    "react": "^16.8.6",
    "react-datepicker": "^2.5.0",
    "react-dom": "^16.8.6",
    "react-redux": "^7.0.3",
    "react-scripts-ts": "3.1.0",
    "react-toastify": "^5.1.0",
    "redux": "^4.0.1",
    "semantic-ui-offline": "^2.4.1",
    "semantic-ui-react": "^0.87.1",
    "socket.io-client": "^2.2.0",
    "winston": "^3.2.1"
  },
  "scripts": {
    "start": "concurrently --kill-others \"npm run desktop\" \"npm run react\"",
    "desktop": "tsc -p tsconfig.electron.json --skipLibCheck && electron .",
    "react": "react-scripts-ts start",
    "build": "react-scripts-ts build",
    "postbuild": "tsc -p tsconfig.electron.json --skipLibCheck",
    "test": "react-scripts-ts test --env=jsdom",
    "eject": "react-scripts-ts eject",
    "postinstall": "electron-rebuild",
    "update-lib-ems": "npm i @the-orange-alliance/lib-ems@latest"
  },
  "devDependencies": {
    "@types/node": "^12.0.0",
    "@types/react": "^16.8.16",
    "@types/react-datepicker": "^2.3.0",
    "@types/react-dom": "^16.8.4",
    "@types/react-redux": "^7.0.8",
    "@types/react-toastify": "^4.0.2",
    "@types/socket.io-client": "^1.4.32",
    "concurrently": "^4.1.0",
    "electron": "^5.0.1",
    "electron-debug": "^2.2.0",
    "electron-rebuild": "^1.8.4",
    "typescript": "^3.4.5"
  },
  "proxy": "http://localhost:4200",
  "homepage": "./"
}
